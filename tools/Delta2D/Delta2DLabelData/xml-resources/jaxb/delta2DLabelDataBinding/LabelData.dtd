<!-- A DTD for label data.--><!ELEMENT labelData (styles | labels | (styles, labels) )><!-- style data *************************************************************** --><!ELEMENT styles              (style*) ><!ELEMENT style               (labelFormat)><!ATTLIST style		layer	                    ( master | sample )             #REQUIRED		image	                    ( singleChannel | dualChannel ) #REQUIRED><!ELEMENT labelFormat         EMPTY><!ATTLIST labelFormat    arrowHeadColor            CDATA                           #IMPLIED    arrowHeadFilled           CDATA                           #IMPLIED    arrowHeadVisible          CDATA                           #IMPLIED        borderColor               CDATA                           #IMPLIED    borderVisible             CDATA                           #IMPLIED    borderWidth               CDATA                           #IMPLIED        strokeColor               CDATA                           #IMPLIED    strokeVisible             CDATA                           #IMPLIED    strokeWidth               CDATA                           #IMPLIED        textBackgroundColor       CDATA                           #IMPLIED    textBackgroundVisible     CDATA                           #IMPLIED    		textColor                 CDATA                           #IMPLIED    textFont                  CDATA                           #IMPLIED><!-- label data *************************************************************** --><!ELEMENT labels              (label*) ><!ELEMENT label               (targetPosition, labelPosition)><!ATTLIST label		id                        CDATA                           #IMPLIED		name	                    CDATA	                          #REQUIRED		format                    ( master | sample )             #IMPLIED		layer	                    ( master | sample )             #IMPLIED><!ELEMENT targetPosition      EMPTY><!ATTLIST targetPosition		x                         CDATA                           #REQUIRED		y                         CDATA                           #REQUIRED><!ELEMENT labelPosition       EMPTY><!ATTLIST labelPosition		x                         CDATA                           #REQUIRED		y                         CDATA                           #REQUIRED>